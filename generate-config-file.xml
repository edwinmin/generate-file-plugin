<!-- 该配置文件为project级，只需要在一个project窗口中建一个generate-config-file.xml文件，位置不限制 -->
<!-- type属性默认为text -->
<config>

    <!-- moduleName为模块名称 eg. edwin-test -->
    <moduleName>Test3</moduleName>

    <!-- packageName为基础包 eg. com.edwin.test—>
    <packageName>com.edwin.test</packageName>

    <!-- sqlMapRelativePath为sqlmap-config.xml和xx.xml所在目录相对resources目录的路径，用于生成XML eg. congfig/sqlmap/activity -->
    <sqlMapRelativePath>config/sqlmap/activity</sqlMapRelativePath>

    <!-- springDaoRelativePath为spring中配置dao bean的XML文件，是相对resources目录的路径 eg. congfig/spring/appcontext-dao-test.xml -->
    <springDaoRelativePath>config/spring/appcontext-test.xml</springDaoRelativePath>

    <!-- springIbatisRelativePath为spring中配置ibatis bean的XML文件，是相对resources目录的路径 eg. congfig/spring/appcontext-db-test.xml -->
    <springIbatisRelativePath>config/spring/appcontext-test.xml</springIbatisRelativePath>

    <!-- table为数据库表，直接复制过来即可 eg. GP_EventFollowNote -->
    <table type="table">
        #table ddl#
    </table>
</config>
